.container {
  display: flex;
}

.container .bodyleft {
  display: flex;
  width: 20%;
}

.container .bodyleft .itme_ {
  flex: 1;
  height: 100%;
  display: flex;
}

.container .bodyleft .itme_ .onever {
  margin-left: 30%;
  width: 10rpx;
  height: 100%;
  background-color: rgba(7, 35, 63, 0.8);
}

.container .bodyleft .itme_ .twover {
  margin-left: 30%;
  width: 10rpx;
  height: 61.8%;
  background-color: rgba(7, 35, 63, 0.8);
}

.container .bodyleft .itme_ .threever {
  margin-left: 30%;
  width: 10rpx;
  height: 38.1924%;
  background-color: rgba(7, 35, 63, 0.8);
}

.container .bodyright {
  width: 70%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container .bodyright .item {
  margin-top: 40rpx;
  border-bottom: 5rpx solid #07233F; 
  width: 500rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
}


.phone_container {
  display: flex;
}

.phone_container .left {
  width:68%;
  box-sizing: border-box;
}

.phone_container .right {
  margin-left: 2%;
  width: 30%;
  box-sizing: border-box;
  display:flex;
  justify-content: center;
  align-items: center;
}

input {
  color: #07233F;
  font-size: 30rpx;
  width: 100%;
  font-weight: 100;
}

.phone_container .right .but {
  border: 10rpx solid #07233F;
  width: 100%;
  height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  font-weight: 100;
} 

.registerbut {
  width: 100%;
  height: 80rpx;
  border: 1rpx solid black;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #FCFBF8;
  background-color: #07233F;
  font-weight: bold;
}


