/* start */
.screenpanalshade {
  position: fixed;
  bottom:0;
  z-index: 119;
  
  background-color: rgba(102, 101, 100, 0.7);
}

.screenpanal {
  
  position: fixed;
  bottom: 0;
  z-index: 120;
  
  background-color: #FCFBF8;
  transition-duration:0.2s;
  
  display: flex;
  flex-direction: column;
}

.screenpanal .head {
  width: 100%;
  height: 80rpx;
  display: flex;
  box-shadow: 0px 1px 5px #D2CFCF;
}

.screenpanal .head .item {
  width: 90rpx;
  height: 100%;
  
  display: flex;
  align-items: center;
  justify-content: center;
}

.screenpanal .head .reset_ {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}


.screenpanal .query_body .line {
  margin: 20rpx;

  height: 80rpx;
  
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
  position: relative;
  border-radius: 10rpx;
  box-shadow: 0px 0px 5px 2px #D2CFCF;
}

.screenpanal .query_body .line .deleteinfo {
  width: 70rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.screenpanal .query_body .line .deleteinfo image {
  width: 30rpx;
  height: 30rpx;
}

.screenpanal .query_body .line .item {
  flex: 1;
  margin-left: 20rpx;
  height: 100%;
}

.screenpanal .query_body .line .list{
  position: absolute;
  top: 80rpx;
  left:0rpx;
  
  background-color: #FCFBF8;

  box-shadow: 0px 0px 5px 2px #D2CFCF;
  
  display: flex;
  flex-direction: column;
}

.screenpanal .query_body .line .nosee {
  z-index: -200;
  visibility: hidden;
}

.screenpanal .query_body .line .list .srcoll_list{
  max-height: 240rpx;

  display: flex;
  flex-direction: column;
}

.screenpanal .query_body .line .list .srcoll_list .item_{
  display: flex;
  align-items: center;
  
  width: 200rpx;
  height: 80rpx;

  margin-left: 20rpx;
  margin-right: 20rpx;
}


.screenpanal .query_body .line .trileveldistri_list {
  z-index: 10;
}

.screenpanal .query_body .line .counter_list {
  z-index: 9;
}

.screenpanal .query_body .line .orderofficer_list {
  z-index: 8;
}

.screenpanal .query_body .line .statusbody {
  display: flex;
  align-items: center;
}

.screenpanal .query_body .line .statusbody .status {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.screenpanal .query_body .line .statusbody .status .check{
  width: 20rpx;
  height: 20rpx;
  border: 1rpx solid #07233F;
}


.screenpanal .query_body .line .statusbody .status .selected{
  background-color: #07233F;
}

.screenpanal .query_body .line .statusbody .veri {
  border-left: 1rpx solid #CBCCCD;
  height: 30rpx;
}

