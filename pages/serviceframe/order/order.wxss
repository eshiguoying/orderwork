.ordercontainer {
  font-size: 30rpx;
  font-weight: 300;
}

.ordercontainer .order_item {
  display: flex;
  margin-top: 20rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  height: 360rpx;
  box-shadow: 0px 0px 5px 2px #D2CFCF;
  border-radius: 10rpx;
  position: relative;
}

.allloadpanel {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  margin-right: 20rpx;
  height: 80rpx;

  color: #9F9E9E;
}

.orderlist_empty {
  display: flex;
  align-items: center;
  justify-content: center;
}

.orderlist_empty .image image {
  width:  162rpx;
  height: 128rpx;
}

.ordercontainer .order_item .deliverytype{
 position: absolute;
 top: 0;
 left: 22rpx;

 width: 40rpx;
 height: 40rpx;

 display: flex;
 align-items: center;
 justify-content: center;
 
 border-bottom-right-radius: 10rpx;
 border-bottom-left-radius: 10rpx
}

.ordercontainer .order_item .GOLD{
  background-color: rgba(174,173,173, 0.2);
  color: #AEADAD;
  font-weight: bold;
}

.ordercontainer .order_item .SPECIAL{
  background-color: rgba(237,102,108, 0.2);
  color: #ED666C;
  font-weight: bold;
}

.ordercontainer .order_item .select_item{
  position: absolute;
  top: 0;
  right: -20rpx;

  width: 80rpx;
  height: 360rpx;

  display: flex;
  align-items: center;
  justify-content: center;

  transition-duration:0.3s;
}

.container_mulchoice {
  width: 80rpx;
  height: 80rpx;

  display: flex;
  justify-content: center;
  align-items: center;

  background-color: #FCFBF8;

  box-shadow: 0px 0px 5px 2px #D2CFCF;

  border-top-left-radius: 10rpx; 
  border-bottom-left-radius: 10rpx; 
}

.ordercontainer .order_item .select_item .container_mulchoice .select_one{
  width: 15rpx;
  height: 15rpx;
  
  border: 4rpx solid #E5E5E5;
  border-radius: 50%;

  display: flex;
  align-items: center;
  justify-content: center;
}

.ordercontainer .order_item .select_item .container_mulchoice  .selected_ {
  background-color: #ED676D;
  border: 4rpx solid #ED676D;
}


.ordercontainer .order_item .order_body {
 flex: 1;
 padding: 20rpx;
}

.order_body .body  {
  width: 100%;
  height: 100%;

  display: flex;
  flex-direction: column;
}

.order_body .body .cusinfo_ {
  height: 100rpx;
  
  display: flex;
  align-items: center;
}

.order_body .body .cusinfo_ .item_ {
  flex: 1;
  display: flex;
  align-items: center;
}


.order_body .body .cusinfo_ .photo{
  width: 40rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.order_body .body .cusinfo_ .photo image {
  width: 32rpx;
  height: 32rpx;
}

.order_body .body .cusinfo_ .info {
  display: flex;
  align-items: center;
  justify-items: center;
}

.order_body .body .cusinfo_ .lugnummould {
  width: 160rpx;
  height: 100%;

  display: flex;
  align-items: center;
}

.order_body .body .cusinfo_ .orderstatus {
  width: 130rpx;
  height: 100%;
  display: flex;
  align-items: center;
}

.order_body .body .item {
  flex: 1;
  display: flex;
  align-items: center;
}

.order_body .body .item .photo {
  width: 40rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.order_body .body .item .send_ {
  font-size: 20rpx;
  color: #ED676D;
}

.order_body .body .item .take_ {
  font-size: 20rpx;
  color: #015C67;
}

.order_body .body .item .photo image {
  width: 25rpx;
  height: 25rpx;
}

.order_body .body .item .info {
  flex: 1;

  height: 100%;
  
  display: flex;
  align-items: center;

  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.order_body .body .item .addr {
  color: #4395FD;
  font-weight: bold;
  text-decoration:underline;
}

.ordercontainer .order_item .space{
  width: 10rpx;
  height: 100%;
}
.ordercontainer .order_item .addr_time_item {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.ordercontainer .order_item .addr_time_item  .addrtype {
  flex: 1;
  display: flex;
  align-items: center;
}

.ordercontainer .order_item .addr_time_item  .addrtype .addrtype_{
  flex: 1;
  height: 80rpx;
  display: flex;
  align-items: center;
}

.ordercontainer .order_item .addr_time_item .time {
  flex: 1;
  display: flex;
}

.ordercontainer .order_item .addr_time_item  .time .sendtime {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
}
.ordercontainer .order_item .addr_time_item .time .taketime {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
}

.ordercontainer .order_item .addr_time_item  .addr {
  flex: 1;
  display: flex;
  flex-direction: column;
}

.ordercontainer .order_item .addr_time_item  .addr .sendaddr{
  flex: 1;
  width: 100%;
  display: flex;
  align-items: center;
}

.ordercontainer .order_item .addr_time_item  .addr .sendaddr .hint{
  width: 35rpx;
  height: 35rpx;
  border: 1rpx solid #CBCCCD;
  display: flex;
  align-items: center;
  justify-content: center;
}

.ordercontainer .order_item .addr_time_item  .addr .sendaddr .detail{
  margin-left: 10rpx;
  width: 450rpx;
  height: 100%;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: flex;
  align-items: center;
  font-weight: 100;
  color: #ACABAB;
}

.ordercontainer .order_item .addr_time_item  .takeaddr{
  flex: 1;
  width: 100%;
  display: flex;
  align-items: center;
}

.ordercontainer .order_item .addr_time_item  .addr .takeaddr .hint{
  width: 35rpx;
  height: 35rpx;
  border: 3rpx solid #CBCCCD;
  display: flex;
  align-items: center;
  justify-content: center;
}

.ordercontainer .order_item .addr_time_item  .addr .takeaddr .detail{
  margin-left: 10rpx;
  width: 450rpx;
  height: 100%;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: flex;
  align-items: center;
  color: #ACABAB;
}

.ordercontainer .order_item .other_item {
  width: 110rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.ordercontainer .order_item .other_item .blongworker{
  flex: 1;
  display: flex;
  align-items: center;

}

.ordercontainer .order_item .other_item .lugnum_ {
  flex: 1;
  display: flex;
  align-items: center;
}

.ordercontainer .order_item .other_item .orderstatus {
  flex: 1;
  display: flex;
  align-items: center;
  font-weight: 400;
}

.ordercontainer .order_item .other_item .PREPAID {
  color: #ED666C;
}

/* 待提取 */
.ordercontainer .order_item .other_item .WAITPICK {
  color: #ED666C;
}

/* 配送中 */
.ordercontainer .order_item .other_item .DELIVERYING {
  color: #005B66;
}

/* 已送达 */
.ordercontainer .order_item .other_item .DELIVERYOVER {
  color: #75B591;
}

/* 已完成 */
.ordercontainer .order_item .other_item .COMPLETED {
  color: #9F68AC;
}

/* 退款中 */
.ordercontainer .order_item .other_item .REFUNDING {
  color: #ACABAB;
}

.floatpanel {
