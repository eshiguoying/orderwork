/* 取 */
.send {
  margin-top: 10rpx;
  margin-left: 5rpx;
  height: 240rpx;
  display: flex;

  background-color: #FCFBF8;
  box-shadow: 1px 2px 4px #E5E5E5,-1px 2px 4px #E5E5E5;
}

.send .left {
  width: 10rpx;
  height: 100%;
  background-color:#ED666C;
}

.send .right {
  flex: 1;
  margin-left: 10rpx;
  height: 240rpx;
  display: flex;
  flex-direction: column
}

.send .right .addrtypecontainer {
  flex: 1;
  display: flex;
  flex-direction: column;
} 

.send .right .addrtypecontainer .addrtypecontainerchild {
  width: 100%;
  height: 70rpx;
  display: flex;
}

.send .right .addrtypecontainer .slidingcontainer {
  flex: 10rpx;
  width: 100%;
} 


.send .right .addrtypecontainer .addrtypecontainerchild .item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.send .right .addrtypecontainer .addrtypecontainerchild .item .content {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 95%;
  height: 85%;
  color: #07233F;
}

.send .right .addrtypecontainer .addrtypecontainerchild .item .on {
  font-size: 35rpx;
}


.send .right .addrtypecontainer .slidingcontainer .sliding {
  background-color: #ED666C;
  width: 15rpx;
  height: 15rpx;
  border-radius: 30%;
}

.sendaddrinfo_hotel {
  transform: translate(112rpx, 0);
  -webkit-transform: translate(112rpx, 0);
  transition-duration:0.4s
}

.sendaddrinfo_residence {
  transform: translate(357rpx, 0);
  -webkit-transform: translate(357rpx, 0);
  transition-duration:0.4s
}

.sendaddrinfo_airport {
  transform: translate(602rpx, 0);
  -webkit-transform: translate(602rpx, 0);
  transition-duration:0.4s
}

.verticalleft {
  display: flex;
  align-items: center;
  justify-items: center;
  width: 100%;
  height: 50%;
  border-left: 1rpx solid #E8E8E7;
  padding-left: 10rpx;
}



.send .right .addrdetailcontainer {
  flex: 1;
  display: flex;
  align-items: center;
}

.send .right .addrdetailcontainer .addrcontainer {
  width: 70%;
  height: 100%;
  display: flex;
  align-items: center;
}

.send .right .addrdetailcontainer .addr {
  width: 98%;
}

.send .right .addrdetailcontainer .doornumcontainer {
  width: 30%;
  height: 100%;
  display: flex;
  align-items: center;
}
 

.send .timecontainer {
  flex: 1;
  display: flex;
  align-items: center;

}


