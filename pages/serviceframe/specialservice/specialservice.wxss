}

.brcode .right {
  flex: 1;
  margin-left: 20rpx;
  height: 100%;

  display: flex;
  align-items: center;
} 


.brcode .right .scanqrbut {
  width: 150rpx;
  height: 60rpx;

  display: flex;
  justify-content: center;
  align-items: center;

  color: #FCFBF8;
  background-color: #EAB4BA;
}


.brcode .right .viewbrbut {
  width: 220rpx;
  height: 60rpx;

  margin-left: 15rpx;

  position: relative;
}

.brcode .right .viewbrbut .lable{ 
  position: absolute;
  bottom: 0rpx;
  left: 0;
  width: 100%;
  height: 100%;

  display: flex;
  justify-content: center;
  align-items: center;
  color: #FCFBF8;
  background-color: #EAB4BA;
  
}

.brcode .right .viewbrbut .nosee {
  z-index: -200;
  visibility: hidden;
}

.brcode .right .viewbrbut .see {
  z-index: 200;
  transition-duration: 1s;
}

.brcode .right  .viewbrbut .qrlist {
  position: absolute;
  bottom: 70rpx;
  left: 0;
  width: 100%;
  border: 2rpx solid #EAB4BA;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.brcode .right  .viewbrbut .qrlist .list {
  width: 100%;
  font-size: 25rpx;

  display: flex;
  align-items: center;
  background-color: #EAB4BA;
}

.brcode .right .viewbrbut .qrlist .list .br {
  flex: 1;
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: bold;
}

.brcode .right  .viewbrbut .qrlist .list .deletebr {
    width: 50rpx;
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 2rpx solid #FCFBF8;
    box-sizing: border-box;
    color: #FCFBF8;
    font-weight: bold;
}

.brcode .right  .viewbrbut .nonsome {
  width: 100%;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #FCFBF8;
  background-color: #EAB4BA;
}

.brcode .right .viewbrbut .cancleviewbr {
  position: absolute;
  bottom: 0rpx;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #EAB4BA;
  color: #FCFBF8;
}


