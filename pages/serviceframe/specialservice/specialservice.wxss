/* 送 */
.take {
  margin-top: 15rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;

  border-radius: 10rpx;

  height: 300rpx;
  display: flex;

  background-color: #FCFBF8;
}

.take .left {
  width: 10rpx;
  height: 100%;
  background-color:#005B66;
}

.take .right {
  flex: 1;
  margin-left: 20rpx;
  height: 300rpx;
  display: flex;
  flex-direction: column
} 

.take .right .addrtypecontainer {
  flex: 1;
  display: flex;
  flex-direction: column;
} 

.take .right .addrtypecontainer .addrtypecontainerchild {
  width: 100%;
  height: 70rpx;
  display: flex;
}

.take .right .addrtypecontainer .slidingcontainer .sliding {
  background-color: #005B66;
  width: 15rpx;
  height: 15rpx;
  border-radius: 30%;
}

.takeaddrinfo_hotel {
  transform: translate(112rpx, 0);
  -webkit-transform: translate(112rpx, 0);
  transition-duration:0.4s
}

.takeaddrinfo_residence {
  transform: translate(357rpx, 0);
  -webkit-transform: translate(357rpx, 0);
  transition-duration:0.4s
}

.takeaddrinfo_airport {
  transform: translate(602rpx, 0);
  -webkit-transform: translate(602rpx, 0);
  transition-duration:0.4s
}

.take .right .addrtypecontainer .addrtypecontainerchild .item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.take .right .addrtypecontainer .addrtypecontainerchild .item .content {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 95%;
  height: 85%;
  color: #07233F;
}

.take .right .addrtypecontainer .addrtypecontainerchild .item .on {
  font-size: 35rpx;
}

.take .right .addrdetailcontainer {
  flex: 1;
  display: flex;
  align-items: center;
}

.take .right .timecontainer {
  flex: 1;
  display: flex;
  align-items: center;
}

.take .right .addrdetailcontainer .addrcontainer {
  width: 70%;
  height: 100%;
  display: flex;
  align-items: center;
}

.take .right .addrdetailcontainer .addr {
  width: 98%;
}

.take .right .addrdetailcontainer .doornumcontainer {
  width: 30%;
  height: 100%;
  display: flex;
  align-items: center;
}

.take input {
  text-overflow:ellipsis;
	white-space:nowrap;
	overflow:hidden;
}

