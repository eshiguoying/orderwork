.container {
  font-size: 30rpx;
  font-weight: 100;

  display: flex;
  flex-direction: column;
}

.querypanel {
  height: 80rpx;

  display: flex;
  align-items: center;
  
  padding: 20rpx;
}

.querypanel ._search{
  flex: 1;

  height: 80rpx;

  display: flex;
  align-items: center;

  padding-left: 20rpx;
  padding-right: 20rpx;

  border-radius: 10rpx;

  box-shadow: 0px 0px 10px 2px #E5E5E5;
}

.querypanel ._search input{
  flex: 1;
}


.querypanel .showforbid{
  margin-left: 20rpx;

  width: 300rpx;
  height: 80rpx;

  border-radius: 10rpx;
  
  /* box-shadow: 0px 0px 10px 2px #E5E5E5; */

  display: flex;
  align-items: center;

  padding-left: 20rpx;
  padding-right: 20rpx;
}

.querypanel .showforbid .but{
  width: 50rpx;
  height: 50rpx;

  border-radius: 10rpx;

  display: flex;
  justify-content: center;
  align-items: center;

  font-weight: bold;
}

.querypanel .showforbid .yes {
  color: #fCFBF8;
  background-color: #005B66; 
}

.querypanel .showforbid .no {
  color: #fCFBF8;
  background-color: #ED666C; 
}

.querypanel .showforbid .lable{
  flex: 1;

  margin-left: 20rpx;
}

.querypanel ._search .deleteinfo {
  width: 70rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.querypanel ._search .deleteinfo image {
  width: 30rpx;
  height: 30rpx;
}

.list {
  flex: 1;

  overflow-y: scroll;

  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-top: 20rpx;
}

.userinfopanel {
  padding: 20rpx;

  margin-bottom: 20rpx;

  display: flex;
  flex-direction: column;

  border-radius: 10rpx;
  
  box-shadow: 0px 0px 10px 2px #E5E5E5;

  position: relative;
}

.userinfopanel .edituser {
  position: absolute;
  right: 20rpx;
  top: 20rpx;

  width: 120rpx;
  height: 60rpx;

  display: flex;
  justify-content: center;
  align-items: center;

  border: 1rpx solid #E5E5E5;
  border-radius: 10rpx;
}

.userinfopanel .startuserbut {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;

  width: 120rpx;
  height: 60rpx;

  display: flex;
  justify-content: center;
  align-items: center;

  border: 1rpx solid #07233F;
  border-radius: 10rpx;
}

.userinfopanel .startuserbut .item {
  flex: 1;
  height: 100%;

  display: flex;
  justify-content: center;
  align-items: center;
}

.userinfopanel .startuserbut .selected {
  color: #fCFBF8;
  background-color: #07233F;
}

.userinfopanel .levelTypetab {
  position: absolute;
  left: 20rpx;
  top: 0rpx;

  width: 60rpx;
  height: 30rpx;

  

  display: flex;
  justify-content: center;
  align-items: center;

  font-size: 28rpx;

  border-radius: 10rpx;
  
  background-color: rgba(174,173,173, 0.2);
  color: #AEADAD;
  font-weight: bold;
  border: 1rpx solid #AEADAD;
}

