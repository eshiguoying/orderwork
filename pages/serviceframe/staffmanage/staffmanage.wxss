.userinfopanel ._body .item{
  display: flex;
  align-items: center;

  height: 60rpx;
}

.userinfopanel ._body .item .lable{
  width: 200rpx;

  display: flex;
  align-items: center;
}

.userinfopanel ._body .item .content{
  flex: 1;

  display: flex;
  align-items: center;
}

/**user.wxss**/
/*顶部搜索*/
.search-b{
  height: 80rpx;
  padding: 20rpx;
  text-align: center;
}
.search-in{
  width: 516rpx;
  height: 66rpx;
  border: 2rpx solid #ddd;
  background-color: #fff;
  color: #333;
  text-align: left;
  padding-left: 40rpx;
  font-size: 30rpx;
  border-radius: 40rpx;
  float: left;
}
.search-btn{
  width: 120rpx;
  height: 70rpx;
  font-size: 30rpx;
  background-color: #fbc400;
  color: #333;
  float: right;
}
/*筛选条件*/
.filter-b{
  clear: both;
  height: 70rpx;
  padding: 0 20rpx;
}
.filter-icon{
  float: left;
  width: 46rpx;
  height: 46rpx;
  border: 2rpx solid #fbc400;
  border-radius: 100%;
}
.filter-icon image{
  width: 36rpx;
  height: 36rpx;
  display: block;
  margin: 5rpx auto;
}
.filter-name{
  line-height: 50rpx;
  color: #333;
  padding-left: 20rpx;
  float: left;
}
/*用户列表*/
.user-list{
  clear: both;
  height: 720rpx;
  padding: 0 20rpx;
  width: 710rpx;
}
.user-item{
  background-color: #fff;
  height: 240rpx;
  border-radius: 20rpx;
  clear: both;
  padding-top: 20rpx;
  margin-top: 20rpx;
}
.user-item.disable{
  background-color: #e1e1e1;
}
.user-level-block{
  width: 90rpx;
  height: 50rpx;
}
.user-level{
  width: 90rpx;
  height: 50rpx;
  background-color: #ffefc7;
  color: #e5b543;
  padding-left: 30rpx;
  font-size: 28rpx;
  line-height: 50rpx;
  border-bottom-right-radius: 30rpx;
  border-top-right-radius: 30rpx;
}
.user-level.h{
  background-color: #fce1d0;
  color: #ff634b;
}
.user-level.m{
  background-color: #fce1d0;
  color: #ff634b;
}
.user-item.disable .user-level.h, .user-item.disable .user-level{
  background-color: #ccc;
  color: #999;
}
.user-item.disable text{
  color: #999;
}
.user-tinfo{
  font-size: 28rpx;
  line-height: 28rpx;
  color: #999;
  padding-top: 40rpx;
}
.user-tinfo text{
  float: left;
}
.user-name{
  width: 200rpx;
  padding-left: 20rpx;
  color: #333;
}
.user-company{
  width: 240rpx;
}
.user-edit{
  width: 110rpx;
}
.user-edit.h{
  color: #2a82e4;
}
.user-open{
  width: 130rpx;
}
.user-binfo{
  clear: both;
  font-size: 28rpx;
  line-height: 36rpx;
  padding-top: 40rpx;
}
.user-binfo text, .user-binfo switch{
  float: left;
}
.user-binfo switch{
  zoom: 0.6;
}
/*登录信息*/
.info-b{
  padding: 20rpx;
}
.info-title{
  font-size: 32rpx;
  padding-bottom: 10rpx;
}
.info-msg text{
  padding-right: 20rpx;
}
.info-msg .edit-btn{
  color: #2a82e4;
}
/*无数据样式*/
.noresult-list{
  font-size: 40rpx;
  color: #333;
  padding-top: 100rpx;
  text-align: center;
}
.noresult-list-img{
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto;
}
.noresult-list-msg{
  padding-top: 50rpx;
}
