.shadepanel {
  background-color: rgba(102, 101, 100, 0.7);
  
  position: fixed;
  bottom: 0;
  z-index: 1001;

  display: flex;
  justify-content: center;
  align-items: center;

  font-size: 25rpx;
  font-weight: 100;
}

.log_body {
  background-color: #FCFBF8;
  border-radius: 10rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}

.close {
  position: absolute;
  right: 0rpx;
  top: 0rpx;
  border: 1rpx solid brown;
}

.close image {
  width: 50rpx;
  height: 50rpx;
}

.orderlist_empty_img {
  width: 162rpx;
  height: 128rpx;
}

.record-item{
  margin-left: 10rpx;
  margin-right: 10rpx;
  height: 80rpx;

  display: flex;
}

.record-item .item{
  height: 80rpx;
}

.record-item .guide_body {
  width: 40rpx;

  display: flex;
  flex-direction: column;
}

.record-item .item .top_body {
  height: 50rpx;

  display: flex;
  justify-content: center;
  align-items: center;

  
}

.record-item .item .top_body .dot {
  width: 20rpx;
  height: 20rpx;

  border-radius: 50rpx;
  background-color: #ddd;
}

.record-item .item .bottom_body {
  flex: 1;

  display: flex;
  justify-content: center;
  align-items: center;
}

.record-item .item .bottom_body .line {
  height: 100%;

  border-left: 1rpx dashed #ddd;
}

.record-item .date_body {
  width: 150rpx;
}

.record-item .date_body {
  font-weight: bold;
}

.record-item .time_body {
  width: 80rpx;
}

.record-item record_body {
  flex: 1;
}