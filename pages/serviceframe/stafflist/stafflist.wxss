.shadepanel {
  width: 100%;
  background-color: rgba(99,99,99,0.3);
  position: fixed;
  bottom: 0;
  z-index: 1001;
  display: flex;
  align-items: center;
  justify-content: center;
}

.staffinfolist {
  width: 600rpx;
  height: 600rpx;
  position: absolute;
  z-index: 701;
  background-color: #FCFBF8;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
}

.shadepanel .staffinfolist .head {
  width: 100%;
  height: 80rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  font-size:30rpx;
  font-weight: 100;
  box-shadow: 1px 1px 4px #E5E5E5;
}

.shadepanel .staffinfolist .head .item {
  width: 90rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.shadepanel .staffinfolist .head .center {
  flex: 1;
}

.shadepanel .staffinfolist .body {
  flex: 1;

  display: flex;
  justify-content: center;
  align-items: center;
}

.shadepanel .staffinfolist .body .stafflistscroll {
  width: 500rpx;
  height: 480rpx;
  font-size: 30rpx;
  font-weight: 100;
}

.shadepanel .staffinfolist .body .stafflistscroll .staffinfo{
  height: 70rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx dashed #CBCCCD;
  box-sizing: border-box;
}

.shadepanel .staffinfolist .body .stafflistscroll .on{
  background-color: rgba(182,184,184, 0.2)
}

.shadepanel .staffinfolist .body .stafflistscroll .staffinfo .item{
  display: flex;
  align-items: center;
}

.shadepanel .staffinfolist .body .stafflistscroll .staffinfo .center{
  flex: 1;
}