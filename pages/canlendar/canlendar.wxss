/* pages/canlendar/canlendar.wxss */
.canlendarBgView{
  width: 97%;
  margin: 0 auto;
}
.weekBgView{
  height: 40px;
  clear: both;
}
.weekView{
  height: 40px;
  line-height: 40px;
  width: 14.2%;
  float: left;
  text-align: center;
}
.canlendarView{
  clear: both;
}
.canlendarTopView{
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.dateEmptyView,.dateView{
  width: 14.2%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  float: left;
}
.today{
  color: #fbc400;
}
.selectDay{
  background: #f6e4a3;
  border-radius: 6rpx;
  color: #333;
}